{
	"name": "de.marianzeis.devtoberfestbadgechecker",
	"version": "1.0.0",
	"description": "UI5 Application: de.marianzeis.devtoberfestbadgechecker",
	"author": "„marianfoo“",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:2": "ui5 build --config ui5-openui5-2.yaml --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"build:opt:2": "ui5 build self-contained --config ui5-openui5-2.yaml --clean-dest --all",
		"start": "ui5 serve --port 8080 -o index.html",
		"start:2": "ui5 serve --config ui5-openui5-2.yaml --port 8080 -o index.html",
		"start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov"
	},
	"devDependencies": {
		"@openui5/types": "1.129.0",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"@ui5/cli": "^4.0.8",
		"eslint": "^9.12.0",
		"karma": "^6.4.4",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^4.0.1",
		"karma-ui5-transpile": "^3.5.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.6.3",
		"ui5-middleware-livereload": "3.1.0",
		"ui5-tooling-transpile": "^3.5.0"
	},
	"dependencies": {
		"node-fetch": "3.3.2"
	}
}
